{
  "manifest_version": 3,
  "name": "Notion empty icon killer",
  "version": "1.0",
  "description": "Remove the empty icon for Notion pages with content but without icons.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://www.notion.so/*"],
      "run_at": "document_end"
    }
  ]
}
